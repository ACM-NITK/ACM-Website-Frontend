ia/'\n\n    const descriptions = this.state.details.smp_des.map((des) => {\n      return (\n        <div>\n          <h2 className=\"space\">\n            <div className=\"details_heading\">{des.sub_heading}</div>\n          </h2>\n          <hr></hr>\n          {/* <h6 className=\"project_text\" >{`${info}`\n          .split('\\n')\n          .map((paragraph, idx) =>\n            <Fragment key={idx}>\n              {paragraph}<br />\n            </Fragment>)}</h6> */}\n      </div>\n      )\n    })\n  \n    return(\n      <div>\n\n        {/* banner area */}\n        <div className=\"banner_background\">\n          <div className=\"banner\">\n            <header className=\"banner_text_area\">\n              <h1 className=\"banner_text1\">{this.state.details.name}</h1>\n            </header>\n          </div>\n        </div>\n\n\n        <div className=\"container\">\n\n          {/* Team section */}\n          <div className=\"space\"></div>\n          <div className=\"row\">\n            <div className=\"col-12 col-md-7\">\n              <Hidden xsDown>\n                <h2 className=\"details_heading\">Details </h2>\n              </Hidden>\n              <Hidden smUp>\n                <h2 className=\"details_heading\">Details </h2>\n              </Hidden>\n              <hr></hr>\n              <Table striped>\n                <tbody>\n                  <Check2 info={this.state.details.mentors} name='Mentors' />\n                  <Check2 info={this.state.details.members} name='Members' />  \n                  <tr>\n                    <th scope=\"row\">Platform of tutoring</th>\n                    <td>{this.state.details.platform_of_tutoring}</td>\n                  </tr>\n                </tbody>\n                </Table>\n            </div>\n            <div className=\"col-12 col-md-5\"></div>\n            {/* <div className=\"col-12 col-md-4\">\n              <Card>\n                <CardActionArea>\n                  <img src={link + this.state.details.display_picture} alt='ex' height=\"250\" width=\"100%\" crop=\"fill\" />\n                </CardActionArea>\n              </Card>\n            </div> */}\n          </div> \n\n\n          {/* Description section */}\n          <div className=\"row\">\n            <div className=\"col-12 col-md-12\">\n              <h2 className=\"space\">\n              <div className=\"details_heading\">Overview</div>\n              </h2>\n              <hr></hr>\n              <h6 className=\"project_text\" >{this.state.details.overview}</h6>\n              <br></br><br></br>\n            </div>\n          </div> \n\n\n          {/* Method section */}\n          <h2 className=\"space\">\n          <div className=\"details_heading\">Method</div>\n          </h2>\n          <hr></hr>\n          <h6 className=\"project_text\">{this.state.details.method}</h6>\n          {descriptions}\n\n\n        </div>\n        <div className=\"space\"></div>\n      </div>\n    )\n  }\n}\n\nexport default SmpDetails;",{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","severity":1,"message":"141","line":68,"column":27,"nodeType":"142","messageId":"143","endLine":68,"endColumn":29},{"ruleId":"144","severity":1,"message":"145","line":9,"column":10,"nodeType":"146","messageId":"147","endLine":9,"endColumn":14},{"ruleId":"136","replacedBy":"148"},{"ruleId":"138","replacedBy":"149"},{"ruleId":"144","severity":1,"message":"150","line":27,"column":10,"nodeType":"146","messageId":"147","endLine":27,"endColumn":19},{"ruleId":"144","severity":1,"message":"151","line":12,"column":8,"nodeType":"146","messageId":"147","endLine":12,"endColumn":12},{"ruleId":"144","severity":1,"message":"152","line":13,"column":8,"nodeType":"146","messageId":"147","endLine":13,"endColumn":22},{"ruleId":"144","severity":1,"message":"153","line":19,"column":7,"nodeType":"146","messageId":"147","endLine":19,"endColumn":16},{"ruleId":"144","severity":1,"message":"154","line":54,"column":10,"nodeTy