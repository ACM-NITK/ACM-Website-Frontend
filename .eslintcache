      </CardActionArea>\n        </Link>\n      </Card>\n    </div>\n    );\n  }\n\n\n\nclass Smp_main extends Component{\n\n  state = { \n    sigs : [], \n  } \n\n  async componentDidMount() { \n    const {REACT_APP_URL} = process.env;\n    try {\n      const res = await axios.get(`${REACT_APP_URL}proposal`) \n      this.setState({ sigs : res.data.sigo }); \n      window.scrollTo(0, 0);\n    }\n    catch(error) {\n      console.log(error);\n    }\n  }\n\n  render () {\n\n      const sigs = this.state.sigs.map((sig) => {\n        if(sig.image) {\n          return (\n              <div key={sig.id} className=\"col-md-3\">\n                  <MediaCard sig={sig} />\n              </div>\n          );\n        }\n        else return(<div></div>)\n      });\n  \n    return(\n      <div>\n\n        {/* banner area */}\n\n        <div className=\"banner_background\">\n            <div className=\"banner\">\n              <header className=\"banner_text_area\">\n                <h1 className=\"banner_text1\">Summer Mentorship Program</h1>\n              </header>\n              {/* <center style = {{marginTop:'50px'}} >\n              <Button variant=\"contained\" color=\"primary\" className=\"register-now-button\" onClick={()=>{window.location.href = 'https://forms.gle/TUKYZuE8xVR6wJju6'}} >Register Here !</Button>\n                \n              </center> */}\n            </div>\n          </div>\n\n        {/* sigs display area */}\n          <div className=\"project_space\">\n            <div className=\"container\">\n              <div className=\"row\"> \n                <div className=\"col-12 col-md-12\">\n                  <h3 className=\"heading\">SMP 2022 <DoubleArrowIcon style={{ fontSize: 30 }} className=\"heading_icon\"/></h3> \n                  <hr className=\"hr\"></hr>\n                </div>\n              </div>\n              <div className=\"row\">\n                {sigs}\n              </div>\n            </div>\n          </div>\n      </div>\n    )\n  }\n}\n\nexport default Smp_main;\n","/home/ranjana/website-acm/src/Components/project_proposal/proposal_details.js",[],"/home/ranjana/website-acm/src/Components/smp/smp_sig.js",[],"/home/ranjana/website-acm/src/Components/smp/smp_details.js",[],"/home/ranjana/website-acm/src/Components/recruitments/Recruitments.js",[],"/home/ranjana/website-acm/src/Components/flipbook/saahithyaMagazine.js",["145"],"import React, {Component} from 'react';\nimport '../../Shared/CSS/home2.css';\nimport '../../Shared/CSS/main.css'\nimport '../../Shared/CSS/flipbook.css'\n// import pdfURL from '../../Shared/magazineFinal.pdf'\n\nclass saahithyaMagazine extends Component{\n    \n    render () {\n\n        return(\n           <div>\n                \n                {/* <div>\n                    <h2 className=\"flipbook-heading\">Saahitya Magazine</h2>\n                </div> */}\n                  \n                  {/* <Announcements items={[{name:'Summer Mentorship', description:'Register for courses now !', link:'/smp'},\n                    {name:'Project Expo', description:'View the project expo !', link:'/expo'},\n                    {name:'Project Proposals', description:'View the project proposals !', link:'/proposal'},\n                    ]}/> */}\n                <div>\n                <iframe width=\"100%\" height=\"1000px\" src=\"https://online.fliphtml5.com/goklc/qiay/\"  seamless=\"seamless\" scrolling=\"no\" frameborder=\"0\" allowfullscreen=\"true\" ></iframe>\n                {/* <iframe src=\"https://cdn.flipsnack.com/widget/v2/widget.html?hash=tiqtywspma\" width=\"100%\" height=\"1000px\" seamless=\"seamless\" scrolling=\"no\" frameBorder=\"0\" allowFullScreen></iframe> */}\n                {/* <iframe src='https://simplebooklet.com/embed.php?wpKey=1xVRshrfvozHEzQ5AHN8bY&source=embed' allowfullscreen width='100%' height='1000px' scrolling='no'></iframe>\n                <iframe src={pdfURL} title=\"description\" allowfullscreen=\"true\" width=\"100%\" height=\"1